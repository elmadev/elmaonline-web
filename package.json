{
  "name": "elmaonline-web",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@fingerprintjs/fingerprintjs": "^3.4.0",
    "@kevinwolf/formal-web": "^0.1.3",
    "@material-ui/core": "4.11.3",
    "@material-ui/icons": "^4.11.2",
    "@material-ui/lab": "^4.0.0-alpha.57",
    "@reach/router": "^1.3.4",
    "@testing-library/jest-dom": "^5.11.4",
    "@testing-library/react": "^11.1.0",
    "@testing-library/user-event": "^12.1.10",
    "apisauce": "^2.1.1",
    "date-fns": "^2.21.1",
    "easy-peasy": "^4.0.1",
    "elma-js": "https://github.com/ville-j/elma-js",
    "fast-memoize": "^2.5.2",
    "flag-icon-css": "^3.5.0",
    "google-map-react": "^2.1.9",
    "level-editor-gui": "https://github.com/elmadev/level-editor-gui.git",
    "lodash": "^4.17.21",
    "moment": "^2.29.4",
    "moment-timezone": "^0.5.33",
    "nanoid": "^3.1.31",
    "query-string": "6.14.1",
    "react": "^17.0.1",
    "react-beautiful-dnd": "^13.1.0",
    "react-dom": "^17.0.1",
    "react-dropzone": "4.2.13",
    "react-google-recaptcha": "^2.1.0",
    "react-helmet-async": "^1.0.9",
    "react-outside-click-handler": "^1.3.0",
    "react-query": "^3.15.2",
    "react-scripts": "4.0.3",
    "react-tiny-bbcode": "^0.1.4",
    "react-window": "^1.8.6",
    "recharts": "^2.2.0",
    "recplayer-react": "^1.5.2",
    "styled-components": "^5.2.3",
    "universal-cookie": "^4.0.4",
    "use-debounce": "^6.0.1",
    "web-vitals": "^0.2.4",
    "yup": "^0.32.9"
  },
  "scripts": {
    "start": "vite",
    "build": "vite build",
    "serve": "vite preview",
    "g": "plop",
    "storybook": "start-storybook -p 6006 -s public",
    "build-storybook": "build-storybook -s public",
    "prettier": "prettier --check src/**/*.js",
    "commit": "git-cz",
    "lint-js": "eslint --ignore-path .gitignore --ignore-pattern \"!**/.*\" ."
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ],
    "plugins": [
      "prettier"
    ],
    "rules": {
      "react-hooks/exhaustive-deps": "off",
      "import/no-anonymous-default-export": "off",
      "no-console": "warn",
      "prettier/prettier": [
        "warn",
        {
          "endOfLine": "auto"
        }
      ]
    }
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@esbuild-plugins/node-globals-polyfill": "^0.2.3",
    "@esbuild-plugins/node-modules-polyfill": "^0.2.2",
    "@storybook/addon-actions": "^6.1.14",
    "@storybook/addon-essentials": "^6.1.14",
    "@storybook/addon-links": "^6.1.14",
    "@storybook/node-logger": "^6.1.14",
    "@storybook/preset-create-react-app": "^3.1.5",
    "@storybook/react": "^6.1.14",
    "@vitejs/plugin-react": "^4.0.4",
    "commitizen": "^4.2.3",
    "compression-webpack-plugin": "6.0.5",
    "cz-customizable": "^6.3.0",
    "eslint-config-prettier": "^7.1.0",
    "eslint-plugin-prettier": "^3.3.1",
    "husky": "^4.3.8",
    "lint-staged": "^10.5.3",
    "plop": "^2.7.4",
    "prettier": "2.2.1",
    "react-app-rewired": "^2.1.8",
    "rollup-plugin-polyfill-node": "^0.12.0",
    "storybook-addon-styled-component-theme": "^2.0.0",
    "vite": "^4.4.12"
  },
  "config": {
    "commitizen": {
      "path": "node_modules/cz-customizable"
    }
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.js": "eslint --cache --fix"
  }
}
